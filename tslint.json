{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-expression": false,
    "no-unused-expression-chai": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prefer-for-of": false,
    "no-var-requires": {
      "severity": "off"
    }
  },
  "rulesDirectory": [
    "tslint-no-unused-expression-chai"
  ]
}